{
  "name": "pattern_language_webpage",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:app": "vite build",
    "build:wasm": "docker run --rm -v $(pwd):/src emscripten/emsdk:3.1.64 ./wasm/build_wasm.sh",
    "build:wasm_win": "docker run --rm -v %cd%:/src emscripten/emsdk:3.1.64 ./wasm/build_wasm.sh",
    "build": "npm run build:wasm && npm install && npm run build:app",
    "build_win": "npm run build:wasm_win && npm install && npm run build:app",
    "preview": "vite preview"
  },
  "dependencies": {
    "plwasm": "file:./wasm/build",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.2.1"
  }
}
